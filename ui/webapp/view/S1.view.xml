<core:View
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	controllerName="uxUnb.controller.S1"
	xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" class="sapUiSizeCompact">
	<Page title="UX Unbound">
		<content>
			<f:SimpleForm id="SimpleFormDisplay354"
			editable="false"
			layout="ResponsiveGridLayout"
			title="Selection"
			labelSpanXL="3"
			labelSpanL="3"
			labelSpanM="3"
			labelSpanS="12"
			adjustLabelSpan="false"
			emptySpanXL="4"
			emptySpanL="4"
			emptySpanM="4"
			emptySpanS="0"
			columnsXL="1"
			columnsL="1"
			columnsM="1"
			singleContainerFullSize="false" >
				<f:content>
					<Label text="Choose Project" required="true" class="sapUiTinyMarginTop"/>
					<ComboBox items="{/ETS_PROJLISTS}">
						<core:Item key="{PROJ_ID}" text="{PROJ_NAME}" />
					</ComboBox>
				</f:content>
			</f:SimpleForm>
		</content>
	</Page>
</core:View>